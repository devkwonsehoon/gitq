{
  "name": "gitzz",
  "version": "0.2.3-beta",
  "description": "Easy get commit data from Github with gitzz",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha test/**/*.ts",
    "build": "tsc",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "coverage": "nyc npm run test && nyc report --reporter=text-lcov | coveralls"
  },
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "git",
    "github",
    "commit",
    "stat"
  ],
  "author": {
    "name": "devkwonsehoon",
    "email": "devkwonsehoon@naver.com",
    "url": "https://velog.io/@devkwonsehoon"
  },
  "homepage": "https://github.com/devkwonsehoon/gitzz",
  "repository": {
    "type": "git",
    "url": "https://github.com/devkwonsehoon/gitzz.git"
  },
  "bugs": {
    "url": "https://github.com/devkwonsehoon/gitzz/issues",
    "email": "devkwonsehoon@naver.com"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.25.0",
    "cheerio": "^1.0.0-rc.10",
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^17.0.18",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.7",
    "coveralls": "^3.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "nyc": {
    "all": true,
    "include": [
      "lib/**/*.ts",
      "lib/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
